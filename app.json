{
  "expo": {
    "name": "Ponto Eletr√¥nico",
    "slug": "ponto-eletronico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3EB489"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            },
            "192.168.1.57": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        }
      },
      "bundleIdentifier": "com.suacompania.pontoeletronico"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3EB489"
      },
      "package": "com.suacompania.pontoeletronico",
      "permissions": [],
      "usesCleartextTraffic": true,
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "primaryColor": "#3EB489",
    "extra": {
      "apiUrl": "http://192.168.1.57/api.php"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}